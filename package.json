{
  "name": "s3-etag",
  "version": "0.0.0",
  "private": false,
  "description": "Generate an S3 ETAG for multipart uploads in Node.js",
  "exports": "./index.js",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "postbuild": "cp package.json README.md LICENSE.md lib/",
    "format": "prettier --write '**/*.{ts,json,svg,md,yml}'",
    "format-check": "prettier --check '**/*.{ts,json,svg,md,yml}'",
    "lint": "eslint src/**/*.ts",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "engines": {
    "node": "^16",
    "npm": "^8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/badsyntax/s3-etag.git"
  },
  "keywords": [
    "s3",
    "etag",
    "hash",
    "md5",
    "aws"
  ],
  "author": "Richard Willis <willis.rh@gmail.com>",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^17.0.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.4.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-github": "^4.3.5",
    "eslint-plugin-jest": "^25.3.0",
    "jest": "^27.4.3",
    "prettier": "2.5.1",
    "ts-jest": "^27.1.0",
    "typescript": "^4.5.2"
  }
}
